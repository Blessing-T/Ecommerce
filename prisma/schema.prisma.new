enum UserRole {
  ADMIN
  CUSTOMER
}

model User {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  name           String?
  email          String?   @unique
  emailVerified  DateTime?
  hashedPassword String?
  image          String?
  role           UserRole  @default(CUSTOMER)
  accounts       Account[]
}